{
    "extends": "@tsconfig/react-native/tsconfig.json",
    "compilerOptions": {
      "baseUrl": ".",
      "paths": {
        "@serv/*": ["src/services/*"],
        "@firebaseServ/*": ["src/services/firebase/*"],
        "@/*": ["src/*"],
        "@components/*": ["src/components/*"],
        "@screens/*": ["src/screens/*"],
        "@api/*": ["src/api/*"],
        "@domain/*": ["src/api/domain/*"],
        "@assets/*":["assets/*"],
        "@dict/*":["assets/dictionaries/*"],
      },
    },
}