{
  "expo": {
    "name": "tttander",
    "slug": "tttander",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/imgs/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/imgs/splashScreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    }, 
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/imgs/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.claualc.tttander"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "3bd5e4ec-9401-4379-8fa9-b1c4c2c1897a"
      }
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notifications_icon.png"
        }
      ]
    ]
  }
}
